<header>
  <div class="header-logo">
    <a href="/main">PianoApp</a>
  </div>
  <ul class="header-menus">
    <li><a href="/testtop">ログアウト</a></li>
  </ul>
</header>


<div class="main users-index">
  <div class="container">
    <h1 class="users-heading">ユーザー一覧</h1>
    <!-- 以下の<% %>を使ってeach文を追加してください -->
    <% @users.each do |user| %>
      <div class="users-index-item">

        <div class="user-left">
          <!-- <img>タグを用いて、プロフィール画像を表示してください-->
          <img src="<%= "/user_images/#{user.image_name}" %>">
        </div>

        <div class="user-right">
          <!-- 以下の<%= %>を使ってユーザー名を表示してください -->
          <%= link_to(user.name, "/users/#{user.id}") %>
        </div>
      </div>
    <!-- 以下の<% %>を使ってeach文のendを追加してください -->
    <% end %>
  </div>
</div>
